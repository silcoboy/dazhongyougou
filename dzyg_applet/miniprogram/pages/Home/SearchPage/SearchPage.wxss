.nosearch{ 
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  height: 100rpx;
  color: #919191;
  letter-spacing: 4rpx;
}

.history-box {
  padding: 0 5px;
}
.history-title {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 40px;
  font-size: 18px;
  font-weight: 600;
}
.history-list {
  display: flex;
}
.history-item{
  margin-top: 5px;
  padding-right: 14px;
  font-size: 18px;
  white-space: nowrap;
}